powershell -ExecutionPolicy Bypass -NoProfile -Command "$root='C:\Win11Upgrade';$ps1url='https://raw.githubusercontent.com/sctcoder1/sctwin11/refs/heads/main/Win11Upgrade.ps1';$baturl='https://raw.githubusercontent.com/sctcoder1/project-711-d/refs/heads/main/Win11Launcher.bat';New-Item -ItemType Directory -Force -Path $root | Out-Null;iwr $ps1url -OutFile (Join-Path $root 'Win11Upgrade.ps1');iwr $baturl -OutFile (Join-Path $root 'Win11Launcher.bat');schtasks /delete /tn Win11Upgrade /f 2>$null | Out-Null;schtasks /create /tn Win11Upgrade /tr 'cmd.exe /c C:\Win11Upgrade\Win11Launcher.bat' /sc once /st 23:59 /RL HIGHEST /RU SYSTEM /F; schtasks /run /tn Win11Upgrade"
