powershell -ep bypass -nop -c "$root='C:\Win11Upgrade';$repo='https://raw.githubusercontent.com/sctcoder1/project-711-d/main';New-Item -Type Directory -Force -Path $root|Out-Null;iwr \"$repo/Win11Launcher.bat\" -OutFile \"$root\Win11Launcher.bat\";iwr \"$repo/Win11Upgrade.ps1\" -OutFile \"$root\Win11Upgrade.ps1\";iwr \"$repo/Win11Reboot.bat\" -OutFile \"$root\Win11Reboot.bat\";iwr \"$repo/Win11Reboot.ps1\" -OutFile \"$root\Win11Reboot.ps1\";iwr \"$repo/Win11Cleanup.bat\" -OutFile \"$root\Win11Cleanup.bat\";iwr \"$repo/Win11Cleanup.ps1\" -OutFile \"$root\Win11Cleanup.ps1\";schtasks /delete /tn Win11Upgrade /f 2>$null|Out-Null;schtasks /delete /tn Win11Reboot /f 2>$null|Out-Null;schtasks /delete /tn Win11Cleanup /f 2>$null|Out-Null;schtasks /create /tn Win11Upgrade /tr 'cmd.exe /c C:\Win11Upgrade\Win11Launcher.bat' /sc once /st 23:59 /RL HIGHEST /RU SYSTEM /F;schtasks /create /tn Win11Reboot /tr 'cmd.exe /c C:\Win11Upgrade\Win11Reboot.bat' /sc once /st 02:00 /RL HIGHEST /RU SYSTEM /F;schtasks /create /tn Win11Cleanup /tr 'cmd.exe /c C:\Win11Upgrade\Win11Cleanup.bat' /sc once /st 02:15 /RL HIGHEST /RU SYSTEM /F;schtasks /run /tn Win11Upgrade;"
