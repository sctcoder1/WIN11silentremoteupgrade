powershell -ep bypass -nop -c "$root='C:\Win11Upgrade';$repo='https://raw.githubusercontent.com/sctcoder1/project-711-d/main';$tomorrow=(Get-Date).AddDays(1).ToString('MM/dd/yyyy');New-Item -Type Directory -Force -Path $root|Out-Null;iwr \"$repo/Win11Launcher.bat\" -OutFile \"$root\Win11Launcher.bat\";iwr \"$repo/Win11Upgrade.ps1\" -OutFile \"$root\Win11Upgrade.ps1\";iwr \"$repo/Reboot.bat\" -OutFile \"$root\Reboot.bat\";iwr \"$repo/Reboot.ps1\" -OutFile \"$root\Reboot.ps1\";iwr \"$repo/Cleanup.bat\" -OutFile \"$root\Cleanup.bat\";iwr \"$repo/Cleanup.ps1\" -OutFile \"$root\Cleanup.ps1\";schtasks /delete /tn Win11Upgrade /f 2>$null|Out-Null;schtasks /delete /tn Win11Reboot /f 2>$null|Out-Null;schtasks /delete /tn Win11Cleanup /f 2>$null|Out-Null;schtasks /create /tn Win11Upgrade /tr 'cmd.exe /c C:\Win11Upgrade\Win11Launcher.bat' /sc once /st 23:59 /sd $tomorrow /RL HIGHEST /RU SYSTEM /F;schtasks /create /tn Win11Reboot /tr 'cmd.exe /c C:\Win11Upgrade\Reboot.bat' /sc once /st 02:00 /sd $tomorrow /RL HIGHEST /RU SYSTEM /F;schtasks /create /tn Win11Cleanup /tr 'cmd.exe /c C:\Win11Upgrade\Cleanup.bat' /sc once /st 02:15 /sd $tomorrow /RL HIGHEST /RU SYSTEM /F;schtasks /run /tn Win11Upgrade;"
